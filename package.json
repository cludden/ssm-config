{
  "name": "@cludden/ssm-config",
  "version": "1.0.0-beta.2",
  "description": "a node.js package that pulls configuration from aws ssm parameter store",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist/* && babel src -d dist",
    "coverage": "nyc --reporter=lcov npm test",
    "lint": "eslint ./lib",
    "postversion": "npm run build",
    "release": "standard-version",
    "sec": "nsp check --output summary",
    "test": "mocha --recursive test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cludden/ssm-config.git"
  },
  "author": "chris ludden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cludden/ssm-config/issues"
  },
  "homepage": "https://github.com/cludden/ssm-config#readme",
  "devDependencies": {
    "aws-sdk": "^2.172.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "faker": "^4.1.0",
    "mocha": "^4.0.1",
    "nsp": "^3.1.0",
    "nyc": "^11.4.1",
    "sinon": "^4.1.3",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "temp-directory": "./coverage/.nyc_output"
  }
}
